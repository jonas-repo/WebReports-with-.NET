
@{
    ViewBag.Title = "Success";
}

<div id="main">
    <!-- Main Content start-->
    <div class="content">
        <div class="container">
            <div class="row">
                <div class="posts-block col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <article>
                        <h3 class="title">Este es tu numero de seguimiento de reporte, conservalo para que puedas ver el estado de tu reporte</h3>
                        <div class="jumbotron">
                            <h1>@ViewBag.num</h1>
                            <p>
                                Asegurate de guardar este numero antes de salir de la página
                            </p>
                        </div>
                    </article>
                </div>
                <!-- Left Section End -->
            </div>
        </div>
        <div class="divider"></div>
    </div>
    <!-- Main Content end-->
</div>
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script>
    $(document).ready(function () {
        document.getElementById("footer").style.marginTop = "21em";
    });
</script>